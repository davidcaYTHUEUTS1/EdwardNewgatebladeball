    for i,v in pairs(AllStore) do
        ShopItem_Tab:AddButton(i,function()
            getgenv().noclip = false;
            stopchest()
            getgenv().noclip = true;
            local twochestz = twochest(v)
            repeat task.wait()until (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).Magnitude <= 5
            getgenv().noclip = false;
            stopchest()
        end)
    end
    
    ------------------------------------------------ Script 
    
    function GetDistance(target)
        return math.floor((target.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)
    end
    
    function equip(typ) --- equip tool
        for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if(v:IsA('Tool'))then
                if(v.ToolTip==typ or v.Name==typ )then
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
                end
            end
        end
    end
    
    function unequip() --- equip tool
        game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
    end
    
    task.spawn(function()
        while wait() do
            pcall(function()
                if getgenv().Config.autofarmlevel then
                    if atsk_z then
                        keydown("Z")
                    end
                    if atsk_x then
                        keydown("X")
                    end
                    if atsk_c then
                        keydown("C")
                    end
                    if atsk_v then
                        keydown("V")
                    end
                end
            end)
        end
    end)
    

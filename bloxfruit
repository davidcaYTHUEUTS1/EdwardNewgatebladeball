    ---Auto Farm Chest
    
    local function getCHEST()
        local dist, thing = math.huge
        for i, v in next, game:GetService("Workspace"):GetChildren() do
            if v:IsA("Model") and v.Name == "Chest" then
                if v:FindFirstChild("Bottom_Frame").Orientation == Vector3.new(0,v:FindFirstChild("Bottom_Frame").Orientation.Y,0) then
                    local mag =
                    (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Hitbox.Position).magnitude
                    if mag < dist then
                    dist = mag
                        thing = v
                    end
                end
            end
        end
        return thing
    end
    spawn(function()--
        while task.wait() do
            pcall(function()
                if getgenv().Config.FarmChest then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getCHEST():FindFirstChild("Hitbox").CFrame * CFrame.new(0,0,1)
                    if (game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.Position - getCHEST():FindFirstChild("Hitbox").Position).Magnitude <= 3 then
                        fireproximityprompt(getCHEST():FindFirstChild("Hitbox").ProximityPrompt,math.huge)
                    end
                end
            end)
        end
    end)
    
    -------------------------
    task.spawn(function()
        while task.wait() do
            pcall(function()
                if getgenv().Config.distance == 'Above' then
                    ModeFarm = CFrame.new(0,4.3,0)* CFrame.Angles(math.rad(-90),0,0)
                elseif getgenv().Config.distance == 'Behind' then
                    ModeFarm = CFrame.new(0,0,4.3)
                elseif getgenv().Config.distance == 'Under' then
                    ModeFarm = CFrame.new(0,-4.3,0) * CFrame.Angles(math.rad(90),0,0)
                end
            end)
        end
    end)

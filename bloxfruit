    function UpdateTheme()
        Library.BackgroundColor=Ass.SettingsMenuBackgroundColor.Value;
        Library.MainColor=Ass.SettingsMenuMainColor.Value;
        Library.AccentColor=Ass.SettingsMenuAccentColor.Value;
        Library.AccentColorDark=Library:GetDarkerColor(Library.AccentColor);
        Library.InlineColor=Ass.SettingsMenuInlineColor.Value;
        Library.OutlineColor=Ass.SettingsMenuOutlineColor.Value;
        Library.FontColor=Ass.SettingsMenuFontColor.Value;
        Library:UpdateColorsUsingRegistry();
    end;
    function SetDefault()
        getgenv().ColorUI = {
            Font = Color3.fromRGB(255, 255, 255),
            Main = Color3.fromRGB(7,7,7),
            Back = Color3.fromRGB(0, 0, 0),
            Accent = Color3.fromRGB(249,7,73),
            Inline = Color3.fromRGB(34,34,34),
            Outline = Color3.fromRGB(23,23,23)
        }
        Ass.SettingsMenuFontColor:SetValueRGB(Color3.fromRGB(255, 255, 255));
        Ass.SettingsMenuMainColor:SetValueRGB(Color3.fromRGB(7,7,7));
        Ass.SettingsMenuBackgroundColor:SetValueRGB(Color3.fromRGB(0, 0, 0));
        Ass.SettingsMenuAccentColor:SetValueRGB(Color3.fromRGB(249,7,73));
        Ass.SettingsMenuInlineColor:SetValueRGB(Color3.fromRGB(34,34,34));
        Ass.SettingsMenuOutlineColor:SetValueRGB(Color3.fromRGB(23,23,23));
        UpdateTheme();
    end;
    function ResetSettings()
        for a,Wb in pairs(Ass)do
            if typeof(Wb.Original)~="nil"then
                Wb:SetValue(Wb.Original);
            end;
        end;
    end;
    

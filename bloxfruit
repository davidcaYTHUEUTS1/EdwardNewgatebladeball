    function killaura(step)
        game:GetService("Players").LocalPlayer.Character.Combat.Punch:FireServer(step)
    end
    
    --[[task.spawn(function()--auto attack
        while task.wait() do
            pcall(function()
                if getgenv().Config.autofarmlevel then
                    for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                        if v:IsA("Tool") then
                            killaura(1)wait()
                            killaura(2)wait()
                            killaura(3)wait()
                            killaura(4)wait()
                            break
                        end
                    end
                end
            end)
        end
    end)]]
    
    spawn(function()
        while task.wait() do
            pcall(function()
                if getgenv().Config.autofarmlevel then
                    CheckQuest()
                    if game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.Enabled == false then
                        getgenv().noclip = true
                        twochest(CFrameQuest)
                        if (CFrameQuest.Position - game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 5 then
                            game.Players.LocalPlayer.PlayerGui.QuestGui.QuestFunction:InvokeServer(PointQuest,LevelQuest);
                            game:GetService("ReplicatedStorage").Replication.ClientEvents.SetSpawnPoint:FireServer(IslandQuest)
                        end
                    elseif game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.Enabled == true then 
                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CFrameQuest.Position).magnitude >= 600 then
                            CheckQuest()
                            getgenv().noclip = true
                            twochest(CFrameQuest)
                        end
                        if game:GetService("Workspace")["NPC Zones"]:FindFirstChild(IslandQuest) then
                            for i,v in pairs(game:GetService("Workspace")["NPC Zones"][IslandQuest].NPCS:GetChildren()) do
                                if string.find(v.Name, Mon) and not string.find(v.Name, "Boss") then
                                    if v.Humanoid.Health > 0 then
                                        if v:FindFirstChild("HumanoidRootPart") then
                                            repeat task.wait()
                                                pcall(function()
                                                    equip(getgenv().Config.weapon)
                                                    getgenv().noclip = true
                                                    if getgenv().Config.autoattack then
                                                        killaura(4)
                                                    else
                                                        game:GetService'VirtualUser':CaptureController()
                                                        game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672)) 
                                                    end
                                                    twochest(v.HumanoidRootPart.CFrame * ModeFarm)
                                                    v.Humanoid.JumpPower = 0
                                                    v.Humanoid.WalkSpeed = 0
                                                    v.Humanoid.Sit = true
                                                    v.HumanoidRootPart.CanCollide = false
                                                    v.HumanoidRootPart.Size = Vector3.new(20,20,20)
                                                    if v.Humanoid:FindFirstChild("Animator") then
                                                        v.Humanoid.Animator:Destroy()
                                                    end
                                                    sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  math.huge)
                                                    v.Humanoid:ChangeState(14)
                                                    v.Humanoid:ChangeState(11)
                                                    spawn(function()
                                                        keydown("Q")
                                                        for i,x in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                                                            if x.Name == "CurrFly" or x.Name == "MaxFly" then
                                                                x.Value = math.huge
                                                            end
                                                        end
                                                    end)
                                                end)
                                            until v.Humanoid.Health <= 0 or not getgenv().Config.autofarmlevel or not v:FindFirstChild("HumanoidRootPart") or not game.Players.LocalPlayer.PlayerGui.QuestGui.Enabled
                                            BringMode = false
                                        end
                                    end
                                else
                                    getgenv().noclip = true
                                    twochest(CFrameMon)
                                end
                            end
                        end
                    end
                end
            end)
        end
    end)
